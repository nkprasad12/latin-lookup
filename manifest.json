{
    "name": "Latin Lookup",
    "description": "Extension to aid in reading Latin texts.",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["contextMenus"],
      "icons": {
        "16": "/images/marcus16.png",
        "32": "/images/marcus32.png",
        "48": "/images/marcus48.png",
        "128": "/images/marcus128.png"
      }
  }